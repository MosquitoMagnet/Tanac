    <SolidColorBrush x:Key="AcceleriderSecondaryTextBrush" Color="{DynamicResource Neutral7Color}"/>
    <SolidColorBrush x:Key="AcceleriderDisableBrush" Color="{DynamicResource Neutral6Color}"/>
    <SolidColorBrush x:Key="AcceleriderBorderBrush" Color="{DynamicResource Neutral5Color}"/>
    <SolidColorBrush x:Key="AcceleriderDividerBrush" Color="{DynamicResource Neutral4Color}"/>
    <SolidColorBrush x:Key="AcceleriderBackgroundBrush" Color="{DynamicResource Neutral3Color}"/>
