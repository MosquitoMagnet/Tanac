<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:Mv.Ui.Converters">
    
    <converters:IsLessThanConverter x:Key="IsLessThanConverter"/>
    <converters:IsGreaterThanConverter x:Key="IsGreaterThanConverter"/>
    <converters:LessThan1000Converter x:Key="LessThan1000Converter"/>
    <converters:UriToImageSourceConverter x:Key="UriToImageSourceConverter"/>
    <converters:StringToImageConverter x:Key="StringToImageConverter"/>
    <converters:PlusConverter x:Key="PlusConverter"/>
    <converters:NumberToDisplayDataSizeConverter x:Key="NumberToDisplayDataSizeConverter" />
    <converters:NotOperator x:Key="NotOperator" />
    <converters:RoleLimitConverter x:Key="RoleRootLimitConverter" Limit="Root"/>
    <converters:RoleLimitConverter x:Key="RoleLimitConverter" Limit="Admin" ></converters:RoleLimitConverter>
    <converters:BoolToVisibilty x:Key="boolVisibilityConverter"/>
    <converters:EnumToStringConverter x:Key="EnumToStringConverter"/>
</ResourceDictionary>